          "fileds": [
            "firstName", "lastName", "email", "phoneNumber"
          ],
          "potentialAction": [
            {
                "@type": "FormSubmitAction",
                "target": {
                  "@type": "EntryPoint",
                  "urlTemplate": "/contact",
                  "actionPlatform": [
                    "http://schema.org/DesktopWebPlatform",
                    "http://schema.org/IOSPlatform",
                    "http://schema.org/AndroidPlatform"
                  ]
                },
                "query-input": "required name=terms, email=terms, telephone=terms, description=terms"
              }
      
          ]
        },
        {
            "@type": "ContactPoint",
            "telephone": "+1-888-888-8888",
            "contactType": "customer service",
            "areaServed": "US",
